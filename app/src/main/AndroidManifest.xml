<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        tools:replace="android:allowBackup"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Unibasic"
        tools:targetApi="31"
        >
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data android:name="DCLOUD_DEBUG" android:value="true"/>
        <meta-data android:name="GETUI_APPID" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="PUSH_APPID" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="MIPUSH_APPID" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="MIPUSH_APPKEY" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="MEIZUPUSH_APPID" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="MEIZUPUSH_APPKEY" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="OPPOPUSH_APPKEY" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="OPPOPUSH_APPSECRET" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="com.huawei.hms.client.appid" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="com.vivo.push.app_id" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="com.hihonor.push.app_id" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="dcloud_unipush_auto_notification" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="GY_APP_ID" android:value="111" tools:replace="android:value"/>
        <meta-data android:name="com.vivo.push.api_key" android:value="111"  tools:replace="android:value"/>
    </application>

</manifest>